{
    "type": "web",
    "url": "http://localhost",
    "port": "3000",
    "db": {
        "dialect": "postgres",

        "env": "DATABASE_URL",

        "hostname": "localhost",
        "port": "54320",

        "name": "motimize",
        "username": "",
        "password": ""
    },
    "redis": {
        "env": "REDIS_URL",

        "hostname": "localhost",
        "port": "6379"
    },
    "queue": {
        "key": "optimization"
    },
    "callback": {
        "params": {
            "timeout": "30000",
            "followRedirect": "true",
            "maxRedirects": "3"
        }
    },
    "uploads_dir": "uploads",
    "body_parser_limit": "25 MB",
    "mime_types": [
        "image/jpeg",
        "image/png"
    ]
